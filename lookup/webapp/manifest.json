{
    "_version": "1.28.0",
    "sap.app": {
        "id": "dlw.lookup",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZEWM_LOOKUP_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "scanOut": {
                "uri": "/sap/opu/odata/sap/ZEWM_SCAN_OUT_SRV/",
                "type": "OData",
                "settings": {
                    "localUri": "localService/ZEWM_SCAN_OUT_SRV/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "transaction": {
                "uri": "/sap/opu/odata/sap/ZEWM_TRANSACTION_HISTORY_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "telus-lookup-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "ZEWM_Lookup",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.84.22",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "dlw.lookup.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "ScanOut": {
                "type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "scanOut",
				"preload": true
            },
            "Transaction": {
                "type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "transaction",
				"preload": true
            },
            "MainModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {}
            },
            "ProductionModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {}
            },
            "SerialNumbersModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "dlw.lookup.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "main",
                    "target": "main"
                },
				{
					"pattern": "productions",
					"name": "productions",
					"target": "productions"
				},
				{
					"pattern": "productions/{productId}/serialNumbers",
					"name": "serialNumbers",
					"target": "serialNumbers"
				}
            ],
            "targets": {
                "main": {
                    "viewType": "XML",
                    "viewLevel" : 1,
                    "viewId": "main",
                    "viewName": "Main"
                },
                "productions": {
					"viewType": "XML",
					"viewName": "Productions",
					"viewId": "productions",
					"viewLevel": 2,
					"transition": "slide"
				},
                "serialNumbers": {
					"viewType": "XML",
					"viewName": "SerialNumbers",
					"viewId": "serialNumbers",
					"viewLevel": 3,
					"transition": "slide"
				}
            }
        },
        "rootView": {
            "viewName": "dlw.lookup.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        }
    }
}